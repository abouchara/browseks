{
    "manifest_version": 3,
    "name": "Br-EKS-IT: Browser EKS extension",
    "version": "0.0.1.2",
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "host_permissions": [
        "*://*.example.org/",
        "http://localhost:8080/",
        "http://localhost:8080/login"
    ],
    "permissions": [
        "storage",
        "activeTab"
    ]
  }
